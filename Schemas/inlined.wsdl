      <xsd:complexType name="SeatRequestType">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="DepartureAirport" type="LocationType"/>
          <xsd:element minOccurs="0" name="ArrivalAirport" type="LocationType"/>
