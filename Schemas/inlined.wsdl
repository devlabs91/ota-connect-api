        <xsd:attribute name="DocIssueCountry" type="ISO3166" use="optional"/>
        <xsd:attribute name="BirthCountry" type="ISO3166" use="optional"/>
        <xsd:attribute name="BirthPlace" type="StringLength1to64" use="optional"/>
