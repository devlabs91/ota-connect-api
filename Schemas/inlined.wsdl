        <xsd:attribute name="IncludedInRate" type="xs:boolean" use="optional"/>
        <xsd:attribute name="IncludedInEstTotalInd" type="xs:boolean" use="optional"/>
        <xsd:attribute name="RateConvertInd" type="xs:boolean" use="optional"/>
      </xsd:complexType>
      <xsd:complexType name="VehicleCoreType">
