            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="99" name="AuthorizedViewer">
                  <xsd:complexType>
                    <xsd:attribute name="ViewerCode" type="StringLength1to16" use="optional"/>
                    <xsd:attribute name="ViewerCarrierCode" type="StringLength1to8" use="optional"/>
