          </xsd:element>
          <xsd:element minOccurs="0" name="PricingSystem">
            <xsd:complexType>
              <xsd:attributeGroup ref="CompanyID_AttributesGroup"/>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="TotalFare">
