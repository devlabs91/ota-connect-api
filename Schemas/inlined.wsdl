        <xsd:attribute name="DocType" type="OTA_CodeType" use="optional"/>
        <xsd:attributeGroup ref="GenderGroup"/>
        <xsd:attributeGroup ref="BirthDateGroup"/>
        <xsd:attributeGroup ref="EffectiveExpireOptionalDateGroup"/>
