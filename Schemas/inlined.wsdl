            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="BaggageAllowance">
            <xsd:complexType>
              <xsd:attributeGroup ref="UnitsOfMeasureGroup"/>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="CabinType" type="CabinType" use="required"/>
