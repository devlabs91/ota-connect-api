              <xsd:attributeGroup ref="TravelerRefNumberGroup"/>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="Airline" type="CompanyNameType"/>
          <xsd:element name="Text" type="StringLength1to64"/>
        </xsd:sequence>
        <xsd:attribute name="Code" type="AlphaLength4" use="optional"/>
