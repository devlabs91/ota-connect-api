              </xsd:sequence>
              <xsd:attribute name="DayOfWeek" type="DayOfWeekType" use="optional"/>
              <xsd:attribute name="MinimumKeep" type="xs:duration" use="optional"/>
              <xsd:attribute name="MaximumKeep" type="xs:duration" use="optional"/>
              <xsd:attribute name="MaximumRental" type="xs:duration" use="optional"/>
              <xsd:attribute name="OvernightInd" type="xs:boolean" use="optional"/>
              <xsd:attribute name="ReturnAllowedInd" type="xs:boolean" use="optional"/>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="RateGuarantee">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="0" name="Description" type="FormattedTextTextType"/>
              </xsd:sequence>
              <xsd:attributeGroup ref="DeadlineGroup"/>
              <xsd:attributeGroup ref="DateTimeSpanGroup"/>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="PaymentRules">
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="PaymentRulesType">
                  <xsd:sequence>
                    <xsd:element maxOccurs="2" minOccurs="0" name="AcceptablePayments">
