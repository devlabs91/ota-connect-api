                <xsd:attribute name="ExtendedPaymentQuantity" type="Numeric1to999" use="optional"/>
                <xsd:attribute name="ExtendedPaymentFrequency" type="TimeUnitType" use="optional"/>
                <xsd:attribute name="AuthorizationCode" use="optional">
